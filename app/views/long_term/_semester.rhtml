<div class="course_container" id="<%= "semesterdiv_#{semester.id}" %>">
    <div class="semester_header"><%= semester.semester %> <%= semester.year %></div>
    <%= render :partial => 'course', :collection => semester.cis_courses, :locals => {:semester => semester.id} %>
</div>
<%= drop_receiving_element("semesterdiv_#{semester.id}", :position => 'bottom', :update => "semesterdiv_#{semester.id}", :url => { :action => 'update_semester'}, :loading => "element.style.display = 'none'", :hoverclass => "drop_hover", :with => "'id=' + encodeURIComponent(element.id) + '&new_semester=' + encodeURIComponent(#{semester.id})") %>
